procdata_ex(["srel=srel(data)", "dma20=dma20(data)", "roll365=roll365(data)", "dma50=dma50(data)", "roll31Data=roll31(data)"])

indexes = [ "sensex index direct tata", "s&p 500 index direct motilal", "direct nasdaq 100 motilal" ]
mfCodes = []
for index in indexes:
	fm, pm = findmatchingmf(index)
	if len(fm) > 0:
		mfCodes.append(fm[0][1])

datas = [ 'data', 'dma20', 'dma50' ]
for data in datas:
	plot_data(data, mfCodes)
show_plot()

# Notice below I am reusing dma20, so use it as I want its old values, before its overwritten
procdata_ex([ "roll31Data=roll31(dma20)", "dma20=dma20(srel)", "dma50=dma50(srel)"])
datas = [ 'srel', 'dma20', 'dma50', 'roll365', 'roll31Data' ]

for mfCode in mfCodes:
	for data in datas:
		plot_data(data, [mfCode])
	show_plot()

